<h3>Child Comp</h3>

<button class="btn btn-primary" @onclick="(()=> SendingMultipleParams(id))">Count</button>


@code {
    private int id = 0;

    [Parameter]
    public EventCallback<EventCallBackModel> OnSendingParams { get; set; }

    private async Task SendingMultipleParams(int id)
    {
        var multileParamatersClass = new EventCallBackModel();
        multileParamatersClass.Param1 = "testing";
        multileParamatersClass.Param2 = 5;

        await OnSendingParams.InvokeAsync(multileParamatersClass);
    }

}
